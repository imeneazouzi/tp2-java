package exemple_array;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class StringMangerTest {

	@Test
	void test() {
		//arrange 
		String ch="hello";
		
		//act
		String result = StringManger.StringReverse(ch);
		
		//assert
		assertEquals(result, "olleh");
		
	}

}
